{
  "name": "jest-sinon",
  "version": "0.0.0-development",
  "description": "Jest assertions for the mocking library Sinon.js",
  "main": "dist/jest-sinon.cjs.js",
  "module": "dist/jest-sinon.esm.js",
  "license": "MIT",
  "keywords": [
    "jest",
    "sinon",
    "mocks",
    "spies",
    "stubs",
    "testing",
    "test",
    "assertions",
    "matchers",
    "tdd"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --coverage",
    "lint": "eslint src"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "jest": ">=24.0.0",
    "sinon": ">=7.0.0"
  },
  "dependencies": {
    "@jest/expect": "^28.1.1",
    "jest-matcher-utils": ">=22.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": ">=1",
    "jest": "^28.1.1",
    "lint-staged": ">=8",
    "prettier": "^2.0.0",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.3.3",
    "sinon": "^9.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --coverage"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djkf/jest-sinon.git"
  }
}
