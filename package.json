{
  "name": "jest-sinon",
  "version": "1.1.0",
  "description": "Jest assertions for the mocking library Sinon.js",
  "main": "./dist/cjs/jest-sinon.js",
  "module": "./dist/esm/jest-sinon.mjs",
  "exports": {
    ".": {
      "import": "./dist/esm/jest-sinon.mjs",
      "require": "./dist/cjs/jest-sinon.js"
    },
    "./assertions": {
      "import": "./dist/esm/assertions.mjs",
      "require": "./dist/cjs/assertions.js"
    }
  },
  "license": "MIT",
  "keywords": [
    "jest",
    "sinon",
    "mocks",
    "spies",
    "stubs",
    "testing",
    "test",
    "assertions",
    "matchers",
    "tdd"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --coverage",
    "lint": "eslint src"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "jest": ">=24.0.0",
    "sinon": ">=7.0.0"
  },
  "dependencies": {
    "jest-matcher-utils": ">=24.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "babel-jest": "^28.1.3",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": ">=8",
    "jest": "^28.1.3",
    "lint-staged": ">=13",
    "prettier": "^2.7.1",
    "rollup": "^2.77.0",
    "rollup-plugin-babel": "^4.3.3",
    "sinon": "^14.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --coverage"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djkf/jest-sinon.git"
  }
}
